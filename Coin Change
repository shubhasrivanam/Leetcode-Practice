class Solution:
    def coinChange(self, coins: List[int], amount: int) -> int:
        dp = [0]
        for i in range (1, amount+1):
            dp.append(amount+1)
        for i in range(0, amount+1):
            for j in range(0, len(coins)):
                if(i>=coins[j]):
                    dp[i] = min(dp[i], 1+dp[i-coins[j]])
        if(dp[amount]>amount):
            return -1
        else:
            return dp[amount]
        
